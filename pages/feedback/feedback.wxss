page {
    position: relative;
}

.slide-image {
    width: 80%;
    margin: 0 auto;
    display: block;
}

swiper {
    height: 400rpx;
    background-color: #2D2D2D;
}

.action {
    margin: 60rpx 50rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.url_input_wrap {
    background: #FFFFFF;
    border: 1rpx solid #979797;
    border-radius: 30px;
    padding: 15rpx 24rpx;
    width: 400rpx;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.url_input {
    width: 300rpx;
    font-size: 12px;
}

.icon {
    width: 28rpx;
    height: 28rpx;
}

.search_btn {
    width: 200rpx;
    padding: 20rpx 0;
    text-align: center;
    font-size: 32rpx;
    color: #302E22;
    background: #FFDF10;
    box-shadow: 0 7px 16px -9px #FFDB10;
    border-radius: 30px;
}

.videobox {
    margin: 0 50rpx;
    text-align: center;
}

.videobox video {
    width: 100%;
    background: rgba(0,0,0,0.50);
    border-radius: 7.2px;
}

.download_btn {
    margin: 0 50rpx;
    background: #FFDF10;
    box-shadow: 0 7px 16px -9px #FFDB10;
    border-radius: 30px;
    text-align: center;
    font-size: 16px;
    color: #302E22;
    padding: 20rpx 0;
    margin-top: 40rpx;
}

.imgbox {
    text-align: center;
    margin: 30px 0;
    margin-top: 10px;
}

.img_placeholder {
    width: 460rpx;
}

.tips {
    text-align: center;
    font-size: 10px;
    color: #979797;
    letter-spacing: 0.5px;
    margin: 40rpx 0;
}

.dialog {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,.7);
    z-index: 99;
}

.d_body {
    z-index: 99999;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    background-color: #fff;
    font-size: 16px;
    color: #333333;
    text-align: center;
    margin: 0 auto;
    border-radius: 5px;
    width: 75%;
}

.d_conent {
    padding: 40rpx;
}

.d_text {
    text-align: center;
    font-size: 14px;
    color: #999999;
    margin-bottom: 10px;
    margin-top: 10px;
}

.d_btn {
    padding: 14rpx 52rpx;
    border-radius: 28px;
    display: inline-block;
}

.dialog_btn {
    display: flex;
    align-items: center;
    justify-content: space-between;
    border-top: 1px solid rgba(0,0,0,0.1);
}

.dialog_btn view {
    width: 50%;
    text-align: center;
    padding: 10px;
}

.cancle {
    border-right: 1px solid rgba(0,0,0,0.1);
}

.auth-btn {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 99;
    background-color: rgba(255,255,255,0);
}

.ques {
    width: 32rpx;
    height: 32rpx;
    margin-right: 10rpx;
}

.tips {
    display: flex;
    align-items: center;
    width: 100%;
    justify-content: center;
}

.link {
    font-size: 24rpx;
    color: blue;
    margin-left: 20rpx;
}

.pop_collect {
    position: fixed;
    right: 5px;
    top: 180rpx;
    width: 220px;
    padding: 5px 20px;
    border-radius: 10px;
    background-color: #FFDF10;
    color: #333;
    font-size: 14px;
    display: flex;
    align-items: center;
}

.pop_collect::before {
    width: 0;
    height: 0;
    content: '';
    border-right: 6px solid transparent;
    border-left: 6px solid transparent;
    border-bottom: 6px solid #FFDF10;
    position: absolute;
    right: 50px;
    top: -6px;
}

.maskPic {
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 0;
    text-align: center;
    background-color: #000000;
    z-index: 999;
}

.maskPic image {
    margin-top: 60px;
    width: 90%;
}